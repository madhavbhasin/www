
 <ion-view>

 <ion-nav-title> Welfare Cases </ion-nav-title>

  <ion-nav-buttons side="right">    <a href="#/tab/welfarecaseNew" class="button button-icon icon ion-plus-round"> </a>    </ion-nav-buttons>

 <ion-content>

   <div class="item-input-inset">
      <label class="item-input-wrapper">
        <i class="icon ion-search placeholder-icon"></i>
        <input type="search" placeholder="Search" ng-model="search.query">
      </label>
      <button class="button button-clear icon ion-close-round" ng-click="clearSearch()"></button>
    </div>

     <ion-refresher on-refresh="doRefreshFromPulldown()" pulling-text="Pull to refresh">    </ion-refresher>

     <ion-list> 
      <ion-item class="item" collection-repeat="wfCase in welfarecases | filter:search.query" type="item-text-wrap"  href="#/tab/welfarecaseDetails/{{wfCase.Id}}" >
        <h3>{{wfCase.Name}}</h3>
      </ion-item> 
    </ion-list>

  </ion-content>
</ion-view>